{
	"name": "greenpeace/planet4-bulgaria",
	"description": "Greenpeace P4 Bulgaria",
	"license": "MIT",
	"require": {
		"greenpeace/planet4-child-theme" : "dev-develop",
		"greenpeace/planet4-master-theme" : "dev-develop",
		"greenpeace/planet4-plugin-blocks" : "dev-develop",
		"greenpeace/planet4-plugin-migrated-urls": "0.0.1",
		"wpackagist-plugin/wp-stateless": "2.1.7"
	},
	"scripts": {
		"core:language": "wp language core install bg_BG; wp language core activate bg_BG",
		"install:plugin-wpimport": "wp plugin install --activate https://raw.githubusercontent.com/greenpeace/planet4-3rdparty-plugins/master/wp-all-import-pro_4.5.4.zip",
		"site:custom": [
			"@core:language",
			"@install:plugin-wpimport"
		]
	}
}
